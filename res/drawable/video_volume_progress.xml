<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape android:shape="rectangle">
            <corners android:radius="1.0px" />
            <stroke android:width="1.0px" android:color="#ffb4b4b4" />
            <gradient android:startColor="#ffb4b4b4" android:endColor="#ffb4b4b4" android:centerY="1.0" android:centerColor="#ffb4b4b4" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape android:shape="rectangle">
                <corners android:radius="1.0px" />
                <gradient android:startColor="#ffffffff" android:endColor="#ffffffff" android:centerY="1.0" android:centerColor="#ffffffff" />
            </shape>
        </clip>
    </item>
</layer-list>